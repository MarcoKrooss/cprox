{
  "name": "cprox",
  "description": "Node express.js redirect, static serve and proxy server",
  "version": "1.4.1",
  "main": "dist/index.js",
  "author": "majo418",
  "license": "MIT",
  "private": false,
  "keywords": [
    "typescript",
    "node",
    "server",
    "api",
    "proxy",
    "static",
    "redirect"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "exec": "node dist/index.js",
    "tsc": "tsc -p tsconfig.json",
    "test": "mocha --require ts-node/register src/test/**/*.test.ts",
    "build": "npm run tsc",
    "start:watch": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run start\"",
    "build:watch": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run build\"",
    "test:watch": "nodemon -w \"${PWD}/src/**/*\" -x \"npm run test\"",
    "exec:watch": "nodemon -w \"${PWD}/dist/**/*\" -x \"npm run exec\"",
    "dev": "npm run start:watch"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/http-proxy": "^1.17.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/serve-static": "^1.13.10",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4"
  },
  "dependencies": {
    "http-proxy": "^1.18.1",
    "selfsigned": "^2.0.0",
    "serve-static": "^1.14.2",
    "typenvy": "^1.2.2"
  }
}
