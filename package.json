{
  "name": "urlprox",
  "displayName": "UrlProx",
  "version": "1.0.0",
  "description": "NodeJS express.js url proxy server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc -p tsconfig.json",
    "tsnode": "ts-node src/index.ts",
    "build": "npm run compile",
    "dev": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run build; npm run start\"",
    "tsdev": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run tsnode\"",
    "start-watch": "nodemon -w \"${PWD}/dist/**/*\" -x \"npm run start\"",
    "build-watch": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run build\"",
    "win:dev": "nodemon --ext \"*.ts\" -w \"./src/**/*\" -x \"npm run build & npm run start\"",
    "win:tsdev": "nodemon --ext \"*.ts\" -w \"./src/**/*\" -x \"npm run tsnode\"",
    "win:start-watch": "nodemon -w \"./dist/**/*\" -x \"npm run start\"",
    "win:build-watch": "nodemon --ext \"*.ts\" -w \"./src/**/*\" -x \"npm run build\""
  },
  "author": "majo418",
  "license": "MIT",
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/http-proxy": "^1.17.8",
    "@types/node": "^16.3.1",
    "@types/serve-static": "^1.13.10",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "http-proxy": "^1.18.1",
    "serve-static": "^1.14.2",
    "typenvy": "^1.2.2"
  }
}
