{
  "name": "urlprox",
  "displayName": "UrlProx",
  "version": "1.0.0",
  "description": "NodeJS express.js url proxy server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc -p tsconfig.json",
    "tsnode": "ts-node src/index.ts",
    "build": "npm run compile",
    "testjs": "node_modules/typescript/bin/tsc -p .; mocha dist/test/**/*.test.js",
    "win:testjs": "node_modules/typescript/bin/tsc -p . & mocha dist/test/**/*.test.js",
    "test": "mocha --require ts-node/register src/test/**/*.test.ts",
    "devjs": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run build; npm run start\"",
    "dev": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run tsnode\"",
    "test-watch": "nodemon -w \"${PWD}/dist/**/*\" -x \"npm run test\"",
    "start-watch": "nodemon -w \"${PWD}/dist/**/*\" -x \"npm run start\"",
    "build-watch": "nodemon --ext \"*.ts\" -w \"${PWD}/src/**/*\" -x \"npm run build\"",
    "win:devjs": "nodemon --ext \"*.ts\" -w \"./src/**/*\" -x \"npm run build & npm run start\"",
    "win:dev": "nodemon --ext \"*.ts\" -w \"./src/**/*\" -x \"npm run tsnode\"",
    "win:test-watch": "nodemon -w \"${PWD}/dist/**/*\" -x \"npm run win:test\"",
    "win:start-watch": "nodemon -w \"./dist/**/*\" -x \"npm run start\"",
    "win:build-watch": "nodemon --ext \"*.ts\" -w \"./src/**/*\" -x \"npm run build\""
  },
  "author": "majo418",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/http-proxy": "^1.17.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.3.1",
    "@types/serve-static": "^1.13.10",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4"
  },
  "dependencies": {
    "http-proxy": "^1.18.1",
    "selfsigned": "^2.0.0",
    "serve-static": "^1.14.2",
    "typenvy": "^1.2.2"
  }
}