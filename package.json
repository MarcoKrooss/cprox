{
  "name": "urlprox",
  "displayName": "UrlProx",
  "version": "1.0.0",
  "description": "NodeJS express.js url proxy server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "tsnode-watch": "nodemon --ext '*.ts' -w ${PWD}/src -x 'npm run tsnode'",
    "compile": "./node_modules/.bin/tsc -p tsconfig.json",
    "tsnode": "./node_modules/ts-node/dist/bin.js src/index.ts",
    "build": "npm run compile",
    "dev": "nodemon --ext '*.ts' -w ${PWD}/src -x 'npm run build; npm run start'",
    "tsdev": "nodemon --ext '*.ts' -w ${PWD}/src -x 'npm run tsnode'",
    "start-watch": "nodemon -w ${PWD}/app -x 'npm run start'",
    "build-watch": "nodemon --ext '*.ts' -w ${PWD}/src -x 'npm run build'"
  },
  "author": "HalsMaulMajo",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.3.1",
    "@types/node-fetch": "^2",
    "@types/ws": "^8.2.2",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.3",
    "morgan": "^1.10.0",
    "node-fetch": "^2",
    "typenvy": "^1.2.2",
    "ws": "^8.4.0"
  }
}
