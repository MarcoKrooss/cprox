{
  "name": "cprox",
  "description": "Node express.js redirect, static serve and proxy server",
  "version": "1.5.3",
  "main": "dist/index.js",
  "author": "majo418",
  "license": "MIT",
  "private": false,
  "keywords": [
    "typescript",
    "node",
    "server",
    "api",
    "proxy",
    "static",
    "redirect"
  ],
  "bin": {
    "cprox": "./bin/cprox",
    "cproxdev": "./bin/cproxdev"
  },
  "repository": {
    "url": "git@github.com:majo418/cprox.git",
    "type": "git"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "start": "ts-node src/index.ts",
    "exec": "node dist/index.js",
    "test": "mocha --require ts-node/register src/test/**/*.test.ts",
    "build": "npm run tsc",
    "start:watch": "nodemon -w ./src -x \"npm run start\" --ext *.ts",
    "build:watch": "nodemon -w ./src -x \"npm run build\" --ext *.ts",
    "test:watch": "nodemon -w ./src -x \"npm run test\" --ext *.ts",
    "exec:watch": "nodemon -w ./dist -x \"npm run exec\"",
    "dev": "npm run start:watch"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/http-proxy": "^1.17.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/serve-static": "^1.13.10",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4"
  },
  "dependencies": {
    "cmdy": "^1.2.5",
    "http-proxy": "^1.18.1",
    "selfsigned": "^2.0.0",
    "serve-static": "^1.14.2",
    "typenvy": "^1.2.2"
  }
}
